<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
<servlet>
  <description>This is the description of my J2EE component</description> 
  <display-name>This is the display name of my J2EE component</display-name> 
  <servlet-name>RegisterServlet</servlet-name> 
  <servlet-class>com.ambow.vsts.web.RegisterServlet</servlet-class> 
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>StockDoServlet</servlet-name>
    <servlet-class>com.ambow.vsts.web.StockDoServlet</servlet-class>
  </servlet>
    <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>StockDoServlet2</servlet-name>
    <servlet-class>com.ambow.vsts.web.StockDoServlet2</servlet-class>
  </servlet>
    <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.ambow.vsts.web.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ReLoginServlet</servlet-name>
    <servlet-class>com.ambow.vsts.web.ReLoginServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>SaleOrBuyServlet</servlet-name>
    <servlet-class>com.ambow.vsts.web.SaleOrBuyServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>AccountServlet</servlet-name>
    <servlet-class>com.ambow.vsts.web.AccountServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>TradeDetailServlet</servlet-name>
    <servlet-class>com.ambow.vsts.web.TradeDetailServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>StockDoServlet</servlet-name>
    <url-pattern>/StockDoServlet</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
    <servlet-name>StockDoServlet2</servlet-name>
    <url-pattern>/StockDoServlet2</url-pattern>
  </servlet-mapping>	
<servlet-mapping>
  <servlet-name>RegisterServlet</servlet-name> 
  <url-pattern>/RegisterServlet</url-pattern> 
  </servlet-mapping>
  <servlet-mapping>
  <servlet-name>LoginServlet</servlet-name> 
  <url-pattern>/LoginServlet</url-pattern> 
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ReLoginServlet</servlet-name>
    <url-pattern>/ReLoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SaleOrBuyServlet</servlet-name>
    <url-pattern>/SaleOrBuyServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AccountServlet</servlet-name>
    <url-pattern>/AccountServlet</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>TradeDetailServlet</servlet-name>
    <url-pattern>/TradeDetailServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
