<?xml version="1.0" encoding='utf-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
<hibernate-mapping>     
    <class name="com.ambow.vsts.entity.Holding" table="holding"  lazy="true" >
   		<id name="id" column="HoldId" ><generator class="identity"/></id>
    	<property name="number" column="number"/>
    	<property name="avgPrice" column="avgPrice"/>

    	<many-to-one name="user" column="UserId" cascade="save-update">
		</many-to-one>
    	
		<many-to-one name="stock" column="StockId" cascade="save-update">
		</many-to-one>
  	
	</class>  
</hibernate-mapping>  