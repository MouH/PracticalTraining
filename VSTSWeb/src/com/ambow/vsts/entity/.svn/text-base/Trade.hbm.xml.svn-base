<?xml version="1.0" encoding='utf-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
<hibernate-mapping>  
    <class name="com.ambow.vsts.entity.Trade" table="trade" lazy="true">  
	    <id name="id" column="TradeId" type="int"><generator class="native"/></id>  
	        <property name="tradeDate" column="TradeDate" type="date"/> 
	        <property name="tradeNumber"  column="TradeNumber" type="int"/>  
	        <property name="tradePrice" column="TradePrice" type="double"/>

			<many-to-one name="tradetype" column="TradeTypeId" cascade="save-update"/>
	        
	        <many-to-one name="stock" column="StockId" cascade="save-update"/>

			<many-to-one name="user" column="UserId" cascade="save-update"/>
			
    </class>  
</hibernate-mapping> 
 