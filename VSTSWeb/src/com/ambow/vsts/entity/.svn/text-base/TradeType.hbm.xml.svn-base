<?xml version="1.0" encoding='utf-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
<hibernate-mapping>  
    <class name="com.ambow.vsts.entity.TradeType" table="tradetype" lazy="true">
	    <id name="id" column="TradeTypeId" type="int"><generator class="native"/></id>  
	        <property name="info" column="Info" type="string"/>
	        
	     <set name="trade" cascade="save-update" lazy="true">
			<key column="TradeTypeId"/>
			<one-to-many class="com.ambow.vsts.entity.Trade"/>
		</set>
    </class>
</hibernate-mapping>
